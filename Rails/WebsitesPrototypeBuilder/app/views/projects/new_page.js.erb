<% if (@page.valid?) %>
	$('#myCarousel').append('<div class="cloudcarousel" id="<%= @page.id %>"><a class="x-button" onclick="deletePage(\'<%= @page.id %>\')" ><div>x</div></a><img src="/assets/emptyPage.jpg" width="" alt="" title="<%= @page.page_name %>" style="position: block; left: 11.521739130434781px; top: 32px; z-index: -20; width: 40px; height: 60px" ondblclick="show(\'<%= @page.id %>\')"></div>');
<% else %>
	alert("الرجاء التأكد من أن اسم الصفحة فريد من نوعه");
<% end %>