<p id="notice"><%= notice %></p>

<H1>Show pages</H1>
<% @pages.each do |page| %>
	  <p id="pages">
	    <b>Page name:</b>
	   	 <%= page.page_name %>
	   	 <br>
	   	 <b>Page html:</b>
	   	 <%= page.html %>
	   	 <br>
	   	 <td><%= link_to 'Destroy', page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	  </p>
	  
	<% end %>

<button onclick="addPage">Add Page</button>



	


