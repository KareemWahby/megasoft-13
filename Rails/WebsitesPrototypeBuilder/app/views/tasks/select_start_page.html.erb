<style type="text/css">
  /* If you want the view to take the full width of the page if not remove it(.container)*/
  .container{
    width: 100%;
    height: 100%;
    margin: 0px;
    padding: 0px;
  }
</style>

<div id="wrapper">

  <div id="content"> 
    <% for @page in @pages do %>
      <%= link_to @page.page_name, :action => 'save_start_page', :page_id => @page %> 
    <%end%>
  </div>
</div>